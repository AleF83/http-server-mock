{
  "name": "http-server-mock",
  "version": "0.1.0",
  "repository": "git@github.com:AleF83/http-server-mock.git",
  "author": "AleF83 <AlexanderFux83@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:image": "docker buildx build -t alef83/http-server-mock .",
    "start": "node dist/index.js",
    "debug": "nodemon",
    "test": "jest --config .jestrc.json",
    "test:unit": "yarn test --testPathPattern=\"src/.*\\.spec\\.ts$\"",
    "test:integration": "yarn test --testPathPattern=\"tests/.*\\.spec\\.ts$\"",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts"
  },
  "dependencies": {
    "fastify": "2.14.1",
    "ramda": "0.27.0",
    "uuid": "8.1.0"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/ramda": "0.27.6",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.6",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.1",
    "typescript": "3.9.3"
  }
}
