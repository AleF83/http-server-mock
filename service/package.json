  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "watch": "tsc -w -p tsconfig.json",
    "start:dev": "nodemon",
    "test": "jest --detectOpenHandles --config .jestrc.json",
    "test:unit": "yarn test --coverage --testPathPattern=\"src/.*\\.spec\\.ts$\"",
    "test:integration": "yarn test --testPathPattern=\"tests/.*\\.spec\\.ts$\""
  },
  "dependencies": {
    "@nestjs/common": "7.2.0",
    "@nestjs/core": "7.2.0",
    "@nestjs/platform-fastify": "7.2.0",
    "fastify": "2.15.0",
    "http-server-mock-common": "0.1.0",
    "raw-body": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "typescript-collections": "1.3.3",
    "uuid": "8.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "7.2.0",
    "@types/jest": "26.0.0",
    "@types/uuid": "8.0.0",
    "axios": "0.19.2",
    "jest": "26.1.0",
    "mock-http": "1.1.0",
    "nodemon": "2.0.4",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
}
